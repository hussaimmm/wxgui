{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "linux C/C++: g++ build active file",
            "command": "/usr/bin/g++",
            "args": [
                "-fdiagnostics-color=always",
                "-g",
                "${fileDirname}/*.cpp",
                "-o",
                "${workspaceFolder}/bulid/${fileBasenameNoExtension}",
                "-I",
                "/usr/local/lib/wx/include/gtk3-unicode-3.2",
                "-I",
                "/usr/local/include/wx-3.2",
                "-I",
                "${workspaceFolder}/include",
                "-DWXUSINGDLL",
                "-D__WXGTK3__",
                "-D__WXGTK__",
                "-L",
                "/usr/local/lib",
                "-pthread",
                "-lwx_gtk3u_xrc-3.2",
                "-lwx_gtk3u_html-3.2",
                "-lwx_gtk3u_qa-3.2",
                "-lwx_gtk3u_core-3.2",
                "-lwx_baseu_xml-3.2",
                "-lwx_baseu_net-3.2",
                "-lwx_baseu-3.2" 

            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        }
    ],
    "version": "2.0.0"
